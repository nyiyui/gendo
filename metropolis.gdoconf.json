		"Remote": "https://github.com/wlmac/just-the-docs",
		"Mappings":[
			{
				"Slug": "CommonHeader",
				"Data": {
					"Title": "{{ site.title }}",
					"Self": "metropolis-jekyll"
				},
				"SourcePath": "./metropolis-header.html",
				"TargetGlob": "./_layouts/default.html"
			}
		]
	},
	{
		"Remote": "https://github.com/wlmac/doodle",
		"Mappings":[
			{
				"Slug": "CommonHeader",
				"Data": {
					"Title": "{{ site.Title }}",
					"Self": "doodle"
				},
				"SourcePath": "./metropolis-header.html",
				"TargetGlob": "./static/*/*.html"
			}
		]
	}
]
